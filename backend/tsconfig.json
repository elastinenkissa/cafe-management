{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "lib": ["es2019"],
    "allowJs": true,
    "noEmit": false,
    "isolatedModules": true,
    "strict": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "outDir": "dist"
  },
}
